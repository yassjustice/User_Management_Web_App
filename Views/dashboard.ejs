<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <link rel="stylesheet" href="../css/dashboard.css">
    <link rel="stylesheet" href="../css/dashboard2.css">
    <link rel="stylesheet" href="../css/dashboard3.css">
    <script defer src="../js/clientdash.js"></script>
</head>
<body>
    <div class="containerback">
    <div class="navi"> <a href="/logout">logout</a></div>
    <div class="card">
        <div class="top-section">
          <div class="border"></div>
          <img src="uploads/<%= admin.image %>" alt="">
          <h3><%= admin.name %></h3>
          <h4><%= admin.age %></h4>
          <h4><%= admin.salary %></h4>
          <input disabled type="password" name="adminpass" id="" value="<%= admin.password %>">
        </div>
        <div class="bottom-section">
          <span class="title">Dashboard</span>
          <div class="row row1">
            <div class="item">
              <span class="big-text"><%= users.length %></span>
              <span class="regular-text">Users</span>
            </div>
            <div class="item">
              <span class="big-text">0</span>
              <span class="regular-text">Employees</span>
            </div>
            <div class="item">
              <span class="big-text">0</span>
              <span class="regular-text">Admins</span>
            </div>
          </div>
          
<button type="button" class="button_Add" id="showForm">
  <span class="button__text">Add Item</span>
  <span class="button__icon"><svg xmlns="http://www.w3.org/2000/svg" width="24" viewBox="0 0 24 24" stroke-width="2" stroke-linejoin="round" stroke-linecap="round" stroke="currentColor" height="24" fill="none" class="svg"><line y2="19" y1="5" x2="12" x1="12"></line><line y2="12" y1="12" x2="19" x1="5"></line></svg></span>
</button>
<!-- the form that is hidden -->
<div id="formContainer" class="hidden">
  <div class="formWrapper">
      <button id="exitButton">X</button>
